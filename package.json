{
	"id": "jerry-slider",
	"name": "圆环slider",
	"version": "1.0.0",
	"description": "基于官方slider内置组件，定义圆环slider",
	"keywords": [
		"slider",
		"circle-slidere"
	],
	"uni-app": {
		"scripts": {
			"weixin:test": {
				"title": "微信测试环境",
				"env": {
					"UNI_PLATFORM": "mp-weixin",
					"APP_ENV": "development"
				},
				"define": {
					"WXCS-PLATFORM": true
				}
			},
			"weixin:pro": {
				"title": "微信生产环境",
				"env": {
					"UNI_PLATFORM": "mp-weixin",
					"APP_ENV": "production"
				},
				"define": {
					"WXSC-PLATFORM": true
				}
			}
		}
	},
	"dcloudext": {
		"category": [
			"前端组件",
			"通用组件"
		]
	},
	"dependencies": {
		"flyio": "^0.6.14",
		"uview-plus": "^3.3.21"
	}
}